{
  "name": "chill",
  "description": "Make websites less distracting. Focus on the content, not on notifications. ",
  "manifest_version": 2,  
  "version": "1.0.0",
  "homepage_url": "https://github.com/levibostian/chill-web",
  "content_scripts": [
    {
      "matches": ["*://*.github.com/*"],
      "js": ["scripts/github.js"]
    }, {
      "matches": ["*://*.slack.com/*"],
      "js": ["scripts/slack.js"],
      "css": ["styles/slack.css"]
    }
  ]
}